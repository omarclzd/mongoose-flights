<%- include('../partials/header') %>



    <table id="list">
      <thead>
        <tr>
          <th>Airport</th>
          <th>Airline</th>
          <th>Flight Number</th>
          <th>Departs</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% flights.forEach(function(m) { %>
          <tr>
            <td><%= m.airport %></td>
            <td><%= m.airline %></td>
            <td><%= m.flightNo %></td>
            <td><%= m.departs %></td>
            <td><a href="/flights/<%= m._id %>">DETAILS</a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/flights/new">Add New Flight</a>

    <%- include('../partials/footer') %>